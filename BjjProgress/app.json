{
  "expo": {
    "name": "BjjProgress",
    "slug": "BjjProgress",
    "version": "1.0.2",
    "orientation": "portrait",
    "scheme": "bjjprogress",
    "icon": "./assets/appicon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "devices": ["iphone", "ipad"],
      "bundleIdentifier": "com.hubertdomagala.bjjprogress",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to let you upload profile photos or scan credit cards for payments."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hubertdomagala.bjjprogress"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withAndroidGradleProperties",
      "expo-font",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "gradleProperties": {
                "org.gradle.jvmargs": "-Xmx4096m -XX:MaxMetaspaceSize=512m",
                "suppressKotlinVersionCompatibilityCheck": "true"
            }
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "16ad6fce-e62b-4dcc-83c7-582123aaedc7"
      }
    }
  }
}
